footer.footer
    .container
        .footer__wrapper
            .footer__block.footer__logo
                a(href='/').footer-logo.logo-box
                    .footer-logo__img.logo-box__img
                        img(src='img/logo.svg')
                    p.footer-logo__text.logo-box__text most
                .footer-contacts
                    .footer-contacts__wrapper
                        a(href='mailto:hello@most.technology').footer-contacts__item.footer-contacts__item_email hello@most.technology
                        a(href='tel:+16507853270').footer-contacts__item.footer-contacts__item_phone +1 650 7853270
            .footer__block.footer__nav
                .footer-nav
                    .footer-nav__item
                        a(href='#').footer-nav__title Product
                        ul.footer-nav__list.footer-list
                            each el in ['Employers','Schools']
                                li.footer-list__item
                                    a(href='#').footer-list__link=el
                            li.footer-list__item.footer-list__item_bottom
                                a(href='#').footer-list__link Cookie policy
                    .footer-nav__item
                        a(href='#').footer-nav__title Pricing
                        ul.footer-nav__list.footer-list
                            each el in ['One-off hiring','Growing business','Hiring plan']
                                li.footer-list__item
                                    a(href='#').footer-list__link=el
                            li.footer-list__item.footer-list__item_bottom
                                a(href='#').footer-list__link Privacy policy
                    .footer-nav__item
                        a(href='#').footer-nav__title Resources
                        ul.footer-nav__list.footer-list
                            each el in ['Blog','Customer stories','Help Center']
                                li.footer-list__item
                                    a(href='#').footer-list__link=el
                    .footer-nav__item
                        a(href='#').footer-nav__title Company
                        ul.footer-nav__list.footer-list
                            each el in ['About us','Careers','Referral program','Investment']
                                li.footer-list__item
                                    a(href='#').footer-list__link=el
            .footer__block.footer__social
                .footer__btns
                    a(href='#').footer__btn.footer__btn_white Request demo
                    a(href='#').footer__btn.footer__btn_black Sign In
                .footer__socials
